  <title>待装列表</title>
  <style>
    nav {
      position: relative;
    }

    .aui-searchbar input[type=search] {
      padding-left: 10px;
    }

    /* main模板部分样式 */
    main {
      padding-top: 2.5rem;
    }
    .tempList {
      padding: 0px 5px;
    }
    .temp_item {
      background-color: #fff;
      padding: 10px 20px 10px 20px;
      margin-bottom: 0.5rem;
      border-radius: 10px;
      border: 2px solid #36D1A5;
    }

    .temp_item i {
      margin: 10px;
    }

    .temp_item div {
      margin-bottom: 10px;
    }

    .title,
    .edit {
      margin: 0 10px;
    }

    .temp_hd {
      color: darkgreen;
      font-weight: bold;
    }

    .temp_hd .edit {
      padding: 2px 20px;
      background-color: #36D1A5;
      color: #fff;
    }

    /* main部分样式结束 */
    .noMore {
            position: absolute;
            bottom: -10px;
            left: 0;
            right: 0;
            height: 5rem;
            color:#fff;
            text-align: center;
            background-color:#36D1A5;
            line-height: 5rem;
        }
  </style>
</head>

<body class="wrap">
  <header class="aui-bar aui-bar-nav">
        <div class="aui-pull-left aui-btn" tapmode onclick="api.closeWin();">
            <span class="fa fa-arrow-left"></span>
        </div>
        <div class="aui-title">待装列表</div>
    </header>
  <nav>
    <!-- 查询业主部分 -->
    <div class="aui-searchbar" id="search">
      <div class="aui-searchbar-input aui-border-radius" tapmode onclick="doSearch()">
        <i class="fa fa-search"></i>
        <form action="javascript:doSearch();">
          <input type="number" placeholder="待装编号" id="search-input">
        </form>
      </div>
      <div class="aui-searchbar-cancel" tapmod>取消</div>
    </div>
  </nav>
  <!-- NOTE: 主体内容部分 -->
  <main>
    <div class="tempList aui-list-item">
