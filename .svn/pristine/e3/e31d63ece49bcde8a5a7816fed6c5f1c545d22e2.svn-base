/*  清除aui-list-item默认边线*/
.aui-list:before,
.aui-list:after,
.aui-list-item:after,
.aui-list.aui-list-in .aui-list-item-inner:after {
  display: none;
}
/* 模板部分样式 */
.tempList {
  position: relative;
  padding: 0px 5px;
  /* background-color: #ccc; */
  background-image: linear-gradient(92deg, #59493f 0%, #dfc7af 100%);

}

.temp_item .dz {
  position: absolute;
  top: 5px;
  right: 5px;
  width: 10px;
  height: 10px;
  border-radius: 50%;
  background-color: #dfc7af;
  box-shadow: -2px -2px 0px #666;
}

.temp_item {
  background-color:#fff;
  padding: 10px 20px 10px 20px;
  margin-bottom: 0.5rem;
  border-radius: 10px;
  border: 2px solid #36D1A5;
  color: #36D1A5;
  -webkit-box-shadow: 2px 2px 0px #666;
}

.temp_item i {
  margin: 10px;
}

.temp_item div {
  margin-bottom: 10px;
}

.title,
.edit {
  margin: 0 10px;
}

.temp_hd {
  color: darkgreen;
  font-weight: bold;
}

.temp_hd .edit {
  padding: 2px 20px;
  background-color: #36D1A5;
  color: #fff;
}

/* 设置区域滚动 */
main {
  overflow : auto;
  -webkit-overflow-scrolling: touch;
  position: static;
  
}